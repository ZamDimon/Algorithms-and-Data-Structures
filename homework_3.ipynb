   "source": [
    "import turtle as t\n",
    "import random as r\n",
    "\n",
    "t.shape(\"turtle\")\n",
    "t.color(\"red\")\n",
    "t.delay(5)\n",
    "\n",
    "for i in range(100) :\n",
    "    d = r.random()\n",
    "    step = r.random()\n",
    "    \n",
    "    if d < 1/2:\n",
    "        t.left(360*d)\n",
    "    else:\n",
    "        t.right(360*(1-d))\n",
    "        t.forward(100*step)\n",
    "t.mainloop()"
   ]
  }
 ],
